{
  "name": "nextjs-ci-cd-github-actions",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Bruno Ten√≥rio <tenoriobn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-prettier": "^5.2.1",
    "next": "^15.0.3",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build && mv ./out ./build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deploy:prod": "yarn deploy:staging --prod",
    "deploy:staging": "yarn build && vercel ./build --confirm --no-clipboard --token=$VERCEL_TOKEN",
    "deploy:preview": "node ./scripts/pullRequestPreview",
    "test:e2e:open": "cypress open",
    "test:e2e": "cypress run --config-file=cypress.config.js"
  },
  "devDependencies": {
    "cypress": "^13.16.1",
    "eslint": "^9",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.1"
  }
